generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
}

model Food {
    id       Int      @id @default(autoincrement())
    nome      String
    descricao String?
    preco       Int
    categoria     Float
    available DateTime @default(now())
    createdAT DateTime @updatedAt

    @@map("food")
}
